{
  "name": "aniside",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "clients/*",
    "servers/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "husky": "^8.0.1",
    "turbo": "^1.4.3"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  }
}
